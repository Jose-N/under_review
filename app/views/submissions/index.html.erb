<div class="tile-box">
    <% @submissions.each do |submission| %>
      <div class="tile">
        <p class="symbol-bar">Symbol</p>
        <%= link_to image_tag(submission.screenshot, class: "submission-tile"), submission %>
        <p class="tile-title"><%= submission.title %></p>
      </div>
    <% end %>
</div>
